<h1> <%= "Articles tagged with #{@tag}" %> </h1>
<ul id='tag-results'>

  <% @results = @tags.find{ |x| x.name == @tag } %>
  <% @results.nil? ? @results = [] : @results = @results.links %>
  <% @results.each do |link| %>
    <li>
      Title: <%= link.title %>
      URL: <%= link.url %>
    </li>
  <% end %>
</ul>
